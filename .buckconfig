[cache]
  mode = dir

[cxx]
  default_platform = iphonesimulator-x86_64
  cflags = -g -std=c11
  cxxflags = -g -std=c++14
  combined_preprocess_and_compile = true

[alias]
  sbjson-ios = //:SBJson-iOS#default,static

[project]
  ide = xcode
  ignore = .buckd, \
           .hg, \
           .git, \
           buck-cache, \
           buck-out

